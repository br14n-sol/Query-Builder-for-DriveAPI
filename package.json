{
  "name": "query-builder-for-driveapi",
  "author": "Brian Fernandez",
  "description": "Easily generate queries for the Google Drive API.",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./lib/index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "./lib"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prebuild": "npm run lint",
    "build": "tsup --config tsup.config.json && fix-tsup-cjs --cwd lib"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "fix-tsup-cjs": "^1.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/br14n-sol/Query-Builder-for-DriveAPI.git"
  },
  "bugs": {
    "url": "https://github.com/br14n-sol/Query-Builder-for-DriveAPI/issues"
  },
  "homepage": "https://github.com/br14n-sol/Query-Builder-for-DriveAPI#readme",
  "keywords": [
    "drive",
    "driveapi",
    "google",
    "googleapis",
    "query",
    "query builder"
  ]
}
